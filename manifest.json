{
  "manifest_version": 3,
  "name": "15min Activity Tracker",
  "version": "0.1.0",
  "description": "Track and manage your activities in 15-minute increments",
  "icons": {
    "16": "public/icons/icon16.png",
    "48": "public/icons/icon48.png",
    "128": "public/icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}
